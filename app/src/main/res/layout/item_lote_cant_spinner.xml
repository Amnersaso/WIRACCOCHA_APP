<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="5dp"
    android:orientation="horizontal">
    <TextView
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:id="@+id/txt_tiproItemLoteCantSpinner"
        android:visibility="invisible" />
    <TextView
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:id="@+id/txt_codproItemLoteCantSpinner"
        android:visibility="invisible" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="prueba"
        android:paddingTop="10dp"
        android:paddingStart="5dp"
        android:paddingBottom="10dp"
        android:id="@+id/txt_loteItemLoteCantSpinner"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="cod"
        android:visibility="invisible"
        android:id="@+id/txt_fechaItemLoteCantSpinner"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:visibility="invisible"
        android:id="@+id/txt_cantItemLoteCantSpinner"/>

</LinearLayout>